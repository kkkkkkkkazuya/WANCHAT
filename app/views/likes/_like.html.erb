<% if post.liked_by?(current_user) %>
   <%= link_to(post_like_path(post), method: :delete, remote: true, id: "like-btn-#{post.id}") do %>
       <i class="fa-lg fas fa-heart icon-btn liked"></i>
   <% end %>
<% else %>
   <%= link_to(post_like_path(post), method: :post, remote: true, id: "like-btn-#{post.id}") do %>
       <i class="fa-lg fas fa-heart icon-btn not-like"></i>
   <% end %>
<% end %>
